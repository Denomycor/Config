[alias]
        last = log -1 HEAD
        lazy = "!f() { git add . && git commit -m "$@";}; f"
        net = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset>
        ls = log --oneline
        amend = commit --amend --no-edit
        rename = commit --amend -m
        search = !git rev-list --all | xargs git grep -F
        cd = checkout
        alias = config --global --edit
        fork = switch -c
        s = status
        b = branch
